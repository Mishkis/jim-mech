{
  "schemaVersion": 1,
  "id": "jimmech",
  "version": "${version}",
  "name": "JimMech",
  "description": "Adds a mech woo made for itsJim competition",
  "authors": [],
  "contact": {},
  "license": "MIT",
  "icon": "assets/jimmech/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "github.mishkis.jimmech.client.JimmechDataGenerator"
    ],
    "client": [
      "github.mishkis.jimmech.client.JimMechClient"
    ],
    "main": [
      "github.mishkis.jimmech.JimMech"
    ]
  },
  "mixins": [
    "jimmech.mixins.json",
    {
      "config": "jimmech.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  }
}
