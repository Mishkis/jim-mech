{
  "schemaVersion": 1,
  "id": "jimmech",
  "version": "${version}",
  "name": "JimMech",
  "description": "Adds a Mech to the game (made for an itsJim modding competition)",
  "authors": ["Mishkis"],
  "contact": {},
  "license": "MIT",
  "icon": "assets/jim_mech/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "github.mishkis.jimmech.client.JimmechDataGenerator"
    ],
    "client": [
      "github.mishkis.jimmech.client.JimMechClient"
    ],
    "main": [
      "github.mishkis.jimmech.JimMech"
    ]
  },
  "mixins": [
    "jimmech.mixins.json",
    {
      "config": "jimmech.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.source": "https://github.com/Mishkis/jim-mech"
      }
    }
  }
}
